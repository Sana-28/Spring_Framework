package com.bridgeit.ShapeImplementation;

import com.bridgeit.Shape.Shape;

/**Geometry and Circle class in which the object of 
 * Circle class is injected as a constructor argument.
 * @author bridgelabz
 *
 */
public class Geometry implements Shape{
	
	//Refrence variable of Interface
	private Shape Shape;

	//Injecting circle object via Constructor
	public Geometry(Shape shape) {
		
		this.Shape= shape;
	}

	public void draw() {
		
	}
}
